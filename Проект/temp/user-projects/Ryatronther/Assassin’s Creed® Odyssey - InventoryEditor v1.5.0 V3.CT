local results = createFoundList(memscan)
results.initialize()
-- print(string.format('found %s results', results.Count))
for i = 0, results.Count - 1, 1 do
  local addressInt = tonumber(results.Address[i], 16)
  local perkInt = addressInt - 0x8
  local perkId = string.format('%016X', perkInt)
  local hashInt = readQword(perkInt + 0x10)
  local hashId = string.format('%016X', hashInt) -- results.Value[i]
  local label = perkHashes[hashId]
  if addressInt and perkInt and perkId and hashId and hashInt and label then
    -- print(string.format('found hashId %s at %s: %s', hashId, perkId, label))
    perkSelections[perkId] = label
  end
end
results.destroy()
memscan.destroy()

local perkSelectionsText = ''
for k,v in spairs(perkSelections, function(t,a,b) return t[b] &lt; t[a] end) do
  perkSelectionsText = perkSelectionsText .. string.format("%s:%s\n", k, v)
end

mrgp = AddressList.getMemoryRecordByDescription('+Inventory Page')
if mrgp then
  for i = 0, mrgp.Count - 1, 1 do
    local mrp = mrgp.Child[i]
    if mrp.Description == 'pSlotSettings' then
      for i = 0, mrp.Count - 1, 1 do
        local mr = mrp.Child[i]
        if string.sub(mr.Description, 1, 5) == 'pPerk' then
          mr.DropDownList.Text = perkSelectionsText
          mr.DropDownReadOnly = false
          mr.DropDownDescriptionOnly = true
          mr.DisplayAsDropDownListItem = true
        end
      end
    end
  end
end

