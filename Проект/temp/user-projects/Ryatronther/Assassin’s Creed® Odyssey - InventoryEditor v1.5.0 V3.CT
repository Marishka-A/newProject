registerSymbol(swap_main)
registerSymbol(GetSwapData)
registerSymbol(DoSwap)
registerSymbol(err)
registerSymbol(donor)
registerSymbol(newitem)

label(swap_main)
label(DoSwap)
label(GetSwapData)
label(donor)
label(newitem)
label(err)

CreateThread(swap_main)
  //rax, rcx, rdx, r8, r9, r10, r11 - volitile (do save)
  //rbx, rbp, rdi, rsi, rsp, r12, r13, r14, r15 - nonvolatile (preserved)

newmem_swap:
////////////////
  GetSwapData:
////////////////
    mov [rsp+08], rbx
    sub rsp, 20
    mov rbx, dqInventorySlotPtr
    cmp qword ptr [rbx], 0
    je long err_donor_A
      mov rax, call_GetInterface_swap
      call rax
      test rax,rax
    je long err_i_invalid_A
      mov [newitem+0], rax  //pItem
      mov rcx,[rax+8]
      shl rcx,20
      sar rcx,3F
      and rcx,[rax]
    je long err_i_init_A
       lea rdx, [rcx+30]
       mov rdx, [rdx]
       lea r8, [rdx+1C]
       mov [newitem+8], r8 //pRariry
       mov r8, dwPlayerLevel
       mov [newitem+10], r8 //pLevel
       mov r8, [rdx+30]
       mov [newitem+18], r8 //pPerk1
       mov r8, [rdx+38]
       mov [newitem+20], r8 //pPerk2
       mov r8, [rdx+40]
       mov [newitem+28], r8 //pPerk3
       mov r8, [rdx+48]
       mov [newitem+30], r8 //pPerk4
         lea rcx, [newitem+18]
         call LevelPerk
         lea rcx, [newitem+20]
         call LevelPerk
         lea rcx, [newitem+28]
         call LevelPerk
         lea rcx, [newitem+30]
         call LevelPerk
       //--
       mov rdx, [rbx]
       lea rdx, [rdx+8]
       mov rdx, [rdx]
       lea rdx, [rdx+20]
       mov rdx, [rdx]
       lea r8, [rdx+8]
       mov qword ptr [donor+00], r8 //ppItem
       lea r8, [rdx+6C]
       mov qword ptr [donor+08], r8 //pRarity
       lea r8, [rdx+74]
       mov qword ptr [donor+10], r8 //pLevel
       lea r8, [rdx+80]
       mov qword ptr [donor+18], r8 //pPerk1
       lea r8, [rdx+88]
       mov qword ptr [donor+20], r8 //pPerk2
       lea r8, [rdx+90]
       mov qword ptr [donor+28], r8 //pPerk3
       lea r8, [rdx+98]
       mov qword ptr [donor+30], r8 //pPerk4
       mov al, 00
       jmp short exit_A
    err_i_invalid_A:
      mov al, 01 // interface is invalid
      jmp short exit_A
    err_donor_A:
      mov al, 02 // donor is not equipped
      jmp short exit_A
    err_i_init_A:
      mov al, 03 // interface is not initialized
      jmp short exit_A
    exit_A:
      add rsp, 20
      mov rbx, [rsp+08]
      ret
      align 10 CC
/////////////
  LevelPerk:
/////////////
    mov r9, [rcx]
    mov rax, [r9+8]
    shl rax, 20
    sar rax, 3F
    and rax, [r9]
    je exit_LevelPerk
      mov r9, [rax+20] //IObjectModifierTierContainer
      mov rax, [r9+8]
      shl rax, 20
      sar rax, 3F
      and rax, [r9]
    je exit_LevelPerk //Container is Empty, leave
      cmp byte ptr [rax+28], 0
      je @f //no override perk, leave
        mov rax, [rax+10]
        mov [rcx], rax
        ret
      @@f:
        mov rdx, dwPerkUpgradeLevel
        mov edx, [rdx]
        sub edx, 1
        shl edx, 3
        mov r8, [rax+1C]
        add r8, edx
        mov r8,[r8]
        mov [rcx],  r8
    exit_LevelPerk:
      ret
//////////
  DoSwap:
//////////
    mov [rsp+08], rbx
    mov [rsp+10], rdx
    mov [rsp+18], rdi
      mov rbx, [donor]
      mov rdx, [newitem]
      mov [rbx], rdx //set new pItem
      mov rbx, [donor+8]
      mov rdx, [newitem+8]
      mov edx, [rdx]
      mov dword ptr [rbx], edx //set new Rarity
      mov rbx, [donor+10]
      mov rdx, [newitem+10]
      mov edx, [rdx]
      mov dword ptr [rbx], edx //set new Level
      mov rbx, [donor+18]
      mov rdx, [newitem+18]
      mov [rbx], rdx //set new pPerk1
      mov rbx, [donor+20]
      mov rdx, [newitem+20]
      mov [rbx], rdx //set new pPerk2
      mov rbx, [donor+28]
      mov rdx, [newitem+28]
      mov [rbx], rdx //set new pPerk3
      mov rbx, [donor+30]
      mov rdx, [newitem+30]
      mov [rbx], rdx //set new pPerk4
    mov rbx, [rsp+08]
    mov rdx, [rsp+10]
    mov rdi, [rsp+18]

    ret
    align 10 CC
//////////////
  swap_main:
//////////////
    sub rsp, 28 //x64 shadow space+align
      call GetSwapData
      mov byte ptr [err], al
      test al, al
      jne exit_B
        call DoSwap
    exit_B:
      add rsp, 28
      ret
    align 10 CC

donor:
  dq 0  // 00: ppItem
  dq 0  // 08: pRariry
  dq 0  // 10: pLevel
  dq 0  // 18: pPerk1
  dq 0  // 20: pPerk2
  dq 0  // 28: pPerk3
  dq 0  // 30: pPerk4
  align 10 CC

newitem:
  dq 0  // 00: pItem
  dq 0  // 08: pRariry
  dq 0  // 10: pLevel
  dq 0  // 18: pPerk1
  dq 0  // 20: pPerk2
  dq 0  // 28: pPerk3
  dq 0  // 30: pPerk4
